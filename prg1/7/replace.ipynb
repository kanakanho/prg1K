   "source": [
    "import glob\n",
    "import re\n",
    "\n",
    "files = glob.glob(\"*.c\")\n",
    "for file in files:\n",
    "    with open(file, \"r\") as f:\n",
    "        content = str(f.read())\n",
    "    content = re.sub(r\"//(.*)\\n\", \"\\n\",content)\n",
    "    with open(f\"./text/{file}\", \"w\") as f:\n",
    "        f.write(content)"
   ]
